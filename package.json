{
  "name": "liground",
  "version": "0.2.0",
  "author": "Johannes Czech (johannes.czech@cs.tu-darmstadt.de)",
  "description": "A mordern Chess Variant Analysis GUI for the 21st century",
  "license": "AGPL-3.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node scripts/build.js && electron-builder",
    "build:dir": "node scripts/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node scripts/build.js",
    "build:web": "cross-env BUILD_TARGET=web node scripts/build.js",
    "dev": "node scripts/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --config webpack/main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --config webpack/renderer.config.js",
    "postinstall": "node scripts/postinstall.js"
  },
  "dependencies": {
    "apexcharts": "^3.24.0",
    "chessgroundx": "^8.3.0",
    "epic-spinners": "^1.1.0",
    "ffish": "^0.6.2",
    "pretty-checkbox-vue": "^1.1.9",
    "vue": "^2.5.16",
    "vue-apexcharts": "^1.6.0",
    "vue-context": "^6.0.0",
    "vue-electron": "^1.0.6",
    "vue-float-menu": "^1.9.0",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.0.1",
    "vue-table-dynamic": "^0.3.3",
    "vuex": "^3.6.0",
    "vuex-electron": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "ajv": "^6.5.0",
    "babel-loader": "^8.2.2",
    "cfonts": "^2.9.0",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^5.1.6",
    "css-loader": "^5.0.1",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^11.1.0",
    "electron-builder": "^22.9.1",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^7.5.0",
    "eslint-webpack-plugin": "^2.4.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.4",
    "multispinner": "^0.2.1",
    "node-loader": "^1.0.2",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue-eslint-parser": "^7.4.1",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^5.7.0",
    "worker-loader": "^3.0.7"
  }
}
